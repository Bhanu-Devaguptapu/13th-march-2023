#include<stdio.h>
int main()
{
int i,n,min=1,max=1;
int a[n];
printf("Enter the value of n:");
scanf("%d",&n);
printf("Enter the values of array:");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
max=a[0];
min=a[0];
for(i=1;i<n;i++){
if(a[i]>max)
max=a[i];
}
for(i=1;i<n;i++){
 if(a[i]<min)
min=a[i];
}
printf(" max= %d",max);
printf(" min = %d",min);
}
